##### general #####
alias lla="ls -lart"
alias gcc=llvm-gcc
alias g="git"

##### ctags #####
alias ctags="`brew --prefix`/bin/ctags"

##### ruby #####
alias be="bundle _1.10.6_ exec"
alias bi="bundle install --path vendor/bundle"
alias rs="rspec"
alias ra="rake"

##### ghq #####
alias cds='cd $(ghq list -p | peco)'
