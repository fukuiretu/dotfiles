export EDITOR=vi
export PATH=$PATH:~/bin
export LANG=ja_JP.UTF-8

##### rbenv #####
if [ -d ~/.rbenv ]; then
  eval "$(rbenv init - zsh)"
  export PATH="~/.rbenv/shims:$PATH"
fi

##### z #####
if which brew > /dev/null; then
  source $(brew --prefix)/etc/profile.d/z.sh
fi

##### hub #####
eval "$(hub alias -s)"
compdef hub=git
